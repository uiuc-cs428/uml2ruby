<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            objectClass="org.eclipse.core.resources.IFile"
            nameFilter="*.uml"
            id="acceleo.ui.popupMenus.contribution.IFile">
         <action
               class="acceleo.ui.popupMenus.AcceleoGenerateUML2RubyAction"
               enablesFor="+"
               id="acceleo.ui.popupMenus.acceleoGenerateUML2RubyAction"
               icon="icons/default.gif"
               label="Generate Ruby Code"
               menubarPath="org.eclipse.acceleo.module.menu"/>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
   </extension>
</plugin>
